          {
            "in": "query",
            "name": "offset",
            "default": 0,
            "type": "integer",
            "description": "The number of items to skip before starting to collect the result"
          },
          {
            "in": "query",
            "name": "limit",
            "default": 10,
            "type": "integer",
            "description": "The numbers of items to return"
          },
          {
            "in": "query",
            "name": "sort",
            "type": "string",
            "description": "Sorting result by price, metro distance",
            "enum": [
              "price",
              "price-desc",
              "metro"
            ]
          },
          {
            "in": "query",
            "name": "rooms[]",
            "type": "array",
            "minItems": 1,
            "maxItems": 5,
            "uniqueItems": true,
            "collectionFormat": "multi",
            "description": "Rooms list for filter: studio(студия), openplan(свободная планировка), 1(1.комн) etc",
            "items": {
              "type": "string",
              "enum": [
                "studio",
                "openplan",
                "1",
                "2",
                "3",
                "4"
              ]
            }
          },
          {
            "in": "query",
            "name": "deadline[]",
            "type": "array",
            "minItems": 1,
            "maxItems": 4,
            "uniqueItems": true,
            "collectionFormat": "multi",
            "description": "Deadline list for filter: 1 - сдан, 2023x - 2023+",
            "items": {
              "type": "string",
              "enum": [
                "1",
                "2021",
                "2022",
                "2023x"
              ]
            }
          },
          {
            "in": "query",
            "name": "otdelka[]",
            "type": "array",
            "minItems": 1,
            "maxItems": 5,
            "uniqueItems": true,
            "collectionFormat": "multi",
            "description": "Deadline list for filter: 1 - Чистовая, 2 - Без отделки, 3-Черновая, 4- Предчистовая, 5-Под ключ",
            "items": {
              "type": "integer",
              "enum": [
                1,
                2,
                3,
                4,
                5
              ]
            }
          },
          {
            "in": "query",
            "name": "build_category_id[]",
            "type": "array",
            "minItems": 1,
            "maxItems": 5,
            "uniqueItems": true,
            "collectionFormat": "multi",
            "description": "Build category list: 1 - Дерево, 5 - Монолит, 14-Монолит-кирпич, 17- Кирпич, 10-Панельный",
            "items": {
              "type": "integer",
              "enum": [
                5,
                14,
                17,
                10
              ]
            }
          },
          {
            "in": "query",
            "name": "metro",
            "type": "array",
            "uniqueItems": true,
            "collectionFormat": "csv",
            "description": "Metro ids for filter",
            "items": {
              "type": "integer"
            }
          },
          {
            "in": "query",
            "name": "firms",
            "type": "array",
            "uniqueItems": true,
            "collectionFormat": "csv",
            "description": "Developer ids for filter",
            "items": {
              "type": "integer"
            }
          },
          {
            "in": "query",
            "name": "okrug",
            "type": "array",
            "uniqueItems": true,
            "collectionFormat": "csv",
            "description": "Okrug ids of the Moscow",
            "items": {
              "type": "integer"
            }
          },
          {
            "in": "query",
            "name": "okrug_district",
            "type": "array",
            "uniqueItems": true,
            "collectionFormat": "csv",
            "description": "District ids of the Moscow",
            "items": {
              "type": "integer"
            }
          },
          {
            "in": "query",
            "name": "okrug_mo",
            "type": "array",
            "uniqueItems": true,
            "collectionFormat": "csv",
            "description": "Okrug ids of the Moscow region",
            "items": {
              "type": "integer"
            }
          },
          {
            "in": "query",
            "name": "district",
            "type": "array",
            "uniqueItems": true,
            "collectionFormat": "csv",
            "description": "District ids of the Moscow region",
            "items": {
              "type": "integer"
            }
          },
          {
            "in": "query",
            "name": "direction",
            "type": "array",
            "uniqueItems": true,
            "collectionFormat": "csv",
            "description": "Highway ids to filter",
            "items": {
              "type": "integer"
            }
          },
          {
            "in": "query",
            "name": "readiness",
            "type": "array",
            "uniqueItems": true,
            "collectionFormat": "csv",
            "description": "Construction stage: 6 - строительство завершено,13 - идет строительство, 34 - идут внутренние работы",
            "items": {
              "type": "integer",
              "enum": [
                6,
                13,
                34
              ]
            }
          },
          {
            "in": "query",
