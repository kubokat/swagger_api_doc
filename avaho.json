    },
    "/flats": {
      "get": {
        "tags": ["flats"],
        "summary": "Get a list of flats",
        "parameters": [
          {
            "in": "query",
            "name": "offset",
            "default": 0,
            "type": "integer",
            "description": "The number of items to skip before starting to collect the result"
