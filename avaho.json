              "type": "string",
              "enum": [
                "эконом-класс",
                "комфорт-клас",
                "бизнес-класс",
                "элит-класс"
              ]
            }
          },
          {
            "in": "query",
            "name": "sell_stage",
            "type": "integer",
            "description": "Sell stage"
          },
          {
            "in": "query",
            "name": "metro-time",
            "type": "integer",
            "description": "Time to metro "
          },
          {
            "in": "query",
            "name": "metro-type",
            "type": "string",
            "description": "Time to metro (driving, transit)"
          },
          {
            "in": "query",
            "name": "apartment",
            "type": "string",
            "description": "Apartment flag: 1 -  апартаменты, without - без апартаментов",
            "enum": [
              "without",
              "1"
            ]
          },
          {
            "in": "query",
            "name": "price_ot",
            "type": "integer",
            "description": "Filter by min price"
          },
          {
            "in": "query",
            "name": "price_do",
            "type": "integer",
            "description": "Filter by max price"
          },
          {
            "in": "query",
            "name": "square_ot",
            "type": "integer",
            "description": "Filter by min square"
          },
          {
            "in": "query",
            "name": "square_do",
            "type": "integer",
            "description": "Filter by max square"
          },
          {
            "in": "query",
            "name": "fence",
            "type": "integer",
            "description": "1 - огороженная территория",
            "enum": [
              1
            ]
          },
          {
            "in": "query",
            "name": "garbage",
            "type": "integer",
            "description": "1 - без мусоропровода",
            "enum": [
              1
            ]
          },
          {
            "in": "query",
            "name": "boxroom",
            "type": "integer",
            "description": "1 - кладовые помещения",
            "enum": [
              1
            ]
          },
          {
            "in": "query",
            "name": "panoram",
            "type": "integer",
            "description": "1 - панорамное остекление",
            "enum": [
              1
            ]
          },
          {
            "in": "query",
            "name": "arc_style_id",
            "type": "integer",
            "description": "1 - лофт",
            "enum": [
              1
            ]
          },
          {
            "in": "query",
            "name": "multilevel",
            "type": "integer",
            "description": "1 -  двухуровневые",
            "enum": [
              1
            ]
          },
          {
            "in": "query",
            "name": "yard_without_cars",
            "type": "integer",
            "description": "1 -  Двор без машин",
            "enum": [
              1
            ]
          },
          {
            "in": "query",
            "name": "mjk",
            "type": "integer",
            "description": "1 - малоэтажные комплексы",
            "enum": [
              1
            ]
          },
          {
            "in": "query",
            "name": "action",
            "type": "integer",
            "description": "1 - скидки, акции",
            "enum": [
              1
            ]
          },
          {
            "in": "query",
            "name": "penthouse",
            "type": "integer",
            "description": "1 - Пентхаусы",
            "enum": [
              1
            ]
          },
          {
            "name": "high",
            "in": "query",
            "type": "integer",
            "description": "1 - Высотки",
            "enum": [
              1
            ]
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/ListingBuilding"
