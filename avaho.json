          {
            "in": "query",
            "name": "limit",
            "default": 10,
            "type": "integer",
            "description": "The numbers of items to return"
          },
          {
            "in": "query",
            "name": "sort",
            "type": "string",
            "description": "Sorting result by square, floor, price (price-desc - sort by descending, price by ascending)",
            "enum": [
              "price",
              "price-desc",
              "flat_square",
              "flat_square_desc",
              "etag",
              "etag_desc"
            ]
          },
          {
            "in": "query",
            "name": "rooms[]",
            "type": "array",
            "minItems": 1,
            "maxItems": 5,
            "uniqueItems": true,
            "collectionFormat": "multi",
            "description": "Rooms list for filter",
            "items": {
              "type": "string",
              "enum": [
                "studio",
                "openplan",
                "1",
                "2",
                "3",
                "4"
              ]
            }
          },
          {
            "in": "query",
            "name": "within",
            "type": "integer",
            "description": "Parent id filter"
          },
          {
            "in": "query",
            "name": "price_ot",
            "type": "integer",
            "description": "Filter by min price"
          },
          {
            "in": "query",
            "name": "price_do",
            "type": "integer",
            "description": "Filter by max price"
          },
          {
            "in": "query",
            "name": "etag_ot",
            "type": "integer",
            "description": "Filter by min floor"
          },
          {
            "in": "query",
            "name": "etag_do",
            "type": "integer",
            "description": "Filter by max floor"
          },
          {
            "in": "query",
            "name": "square_ot",
            "type": "integer",
            "description": "Filter by min square"
          },
          {
            "in": "query",
            "name": "square_do",
            "type": "integer",
            "description": "Filter by max square"
          },
          {
            "in": "query",
            "name": "firm",
            "type": "integer",
            "description": "Filter by seller"
          },
          {
            "in": "query",
            "name": "korp_ids[]",
            "type": "array",
            "uniqueItems": true,
            "collectionFormat": "multi",
            "description": "Blocks id list for filter",
            "items": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string"
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "items": {
                      "type": "array",
                      "items": {
                        "$ref": "#/definitions/ListingFlats"
                      }
                    },
                    "total": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
